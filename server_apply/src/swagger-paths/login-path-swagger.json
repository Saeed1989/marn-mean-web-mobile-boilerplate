{
  "paths": {
    "/auth/login": {
      "post": {
        "tags": [
          "Log in"
        ],
        "summary": "Log in",
        "parameters": [
          {
            "name": "input",
            "in": "body",
            "description": "log in credentials",
            "schema": {
              "$schema": "http://json-schema.org/draft-04/schema#",
              "type": "object",
              "properties": {
                "username": {
                  "type": "string",
                  "example": "saeed.sharman"
                },
                "password": {
                  "type": "string",
                  "example": "password1"
                }
              },
              "required": []
            }
          }
        ],
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "Returns the result",
            "schema": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}