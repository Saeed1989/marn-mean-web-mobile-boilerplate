    }
  },
  "host": "localhost:5001",
  "basePath": "/api",
  "tags": [
    {
      "name": "RBAC",
      "description": "API for the system"
    }
  ],
  "schemes": ["http"],
  "consumes": ["application/json"],
  "produces": ["application/json"],
  "paths": {
    "/data/search": {
      "post": {
        "tags": ["Data by catagory"],
        "summary": "Checks the data list for a catagory",
        "parameters": [
          {
            "name": "input",
            "in": "body",
            "description": "Data we want to check",
            "schema": {
              "$schema": "http://json-schema.org/draft-04/schema#",
              "type": "object",
              "properties": {
                "searchText": {
                  "type": "string"
                }               
              },
              "required": []
            }
          }
        ],
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "Returns the result",
            "schema": {
              "type": "string"
            }
          }
        }
      }
    },
    "/data/{id}": {
      "get": {
        "tags": ["Data by id"],
        "summary": "Checks the data of a id",
        "parameters": [
          {  
            "name":"id",
            "in":"path",
            "required":true,
            "type":"string",
            "format":"string"
         }
        ],
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "Returns the result",
            "schema": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}
